{
    "name": "nashgao/mac-parser",
    "description": "A Library that parses mac address",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "nashgao",
            "email": "nashgao1992@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=9.5",
        "friendsofphp/php-cs-fixer": "~3.0"
    },
    "autoload": {
        "psr-4" : {
            "Nashgao\\MacParser\\": "src"
        }
    },
    "autoload-dev" : {
        "psr-4": {
            "Nashgao\\Testing\\": "test"
        }
    },
    "minimum-stability": "dev",
    "scripts": {
        "cs-fix": "php-cs-fixer fix $1",
        "test": "phpunit -c phpunit.xml --colors=always",
        "test-group": "phpunit --prepend test/bootstrap.php -c phpunit.xml --color=always --group"
    }
}
